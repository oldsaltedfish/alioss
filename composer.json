{
    "name": "wuliaowyh/alioss",
    "description": "Alibaba Cloud OSS laravel package",
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Wuliaowyh\\AliOss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wuliaowyh\\AliOss\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wuliaowyh\\AliOss\\OssServiceProvider"
            ],
            "aliases": {
                "Oss": "Wuliaowyh\\AliOss\\Facades\\Oss"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wyh",
            "email": "rmzf@qq.com"
        }
    ],
    "require": {
        "aliyuncs/oss-sdk-php": "^2.3"
    }
}
